{
	"name": "blueways/bw-jsoneditor",
	"type": "typo3-cms-extension",
	"description": "Adds a JSON Form Editor type to the TYPO3 Backend. View and edit JSON data with code formatting and syntax highlighting.",
	"license": "GPL-2.0-or-later",
	"keywords": [
		"TYPO3",
		"JSON"
	],
	"require-dev": {
		"typo3/cms-base-distribution": "^13.0",
		"helhum/typo3-console": "^8.0"
	},
	"require": {
		"typo3/cms-core": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0 || ^13.0"
	},
	"authors": [
		{
			"name": "Maik Schneider",
			"email": "schneider.maik@me.com",
			"role": "Developer"
		}
	],
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": "public",
			"extension-key": "bw_jsoneditor"
		}
	},
	"config": {
		"vendor-dir": "vendor",
		"allow-plugins": {
			"typo3/cms-composer-installers": true,
			"typo3/class-alias-loader": true,
			"helhum/typo3-console-plugin": true
		}
	},
	"autoload": {
		"psr-4": {
			"Blueways\\BwJsoneditor\\": "Classes/"
		}
	}
}
